    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.10.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "wnumb": "^1.2.0"
  },
  "devDependencies": {
